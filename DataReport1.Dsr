Private Sub DataReport_Initialize()
    Clientes
    Factura
    If RsCliente.State = 1 Then RsCliente.Close
    With Form1
        Dim buscar As String
        buscar = "%" & .lblid.Caption & "%"
    End With
        RsCliente.Open "SELECT *FROM Cliente Where [Id_Cliente]Like '" & buscar & "'"
        If RsCliente.EOF = False Then
            Set DataReport1.DataSource = RsCliente
        Else
            Exit Sub
        End If
    If RsFactura.State = 1 Then RsFactura.Close
    With Form1
        buscar = "%" & .lblfactura.Caption & "%"
    End With
        RsFactura.Open "SELECT *FROM Factura Where [Id_Factura]Like '" & buscar & "'"
        If RsFactura.EOF = False Then
            Set DataReport1.DataSource = RsCliente
        Else
            Exit Sub
        End If
End Sub
